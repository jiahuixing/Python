<?xml   version="1.0"   encoding="utf-8"?>

<shell>

    <!--push command-->
    <commands type="push">
        <command>adb -s %s shell rm %s</command>
        <command>adb -s %s push %s %s</command>
        <command>adb -s %s reboot</command>
    </commands>

    <!--reboot command-->
    <commands type="reboot">
        <command>adb -s %s reboot</command>
        <command>adb -s %s reboot recovery</command>
        <command>adb -s %s reboot bootloader</command>
    </commands>

    <!--flash command-->
    <commands type="flash">
        <command>fastboot -s %s flash</command>
        <command>fastboot -s %s reboot</command>
    </commands>

    <!--file paths for file account_preview-->
    <file_paths type="account_preview">
        <file_path type="rm_from_path">/data/system/</file_path>
        <file_path type="push_termini_path">/data/system/</file_path>
    </file_paths>

    <!--file paths for Music apk-->
    <file_paths type="apk">
        <file_path type="rm_from_path">/system/app/Music.*</file_path>
        <file_path type="push_termini_path">/system/app/Music.apk</file_path>
    </file_paths>

</shell>
